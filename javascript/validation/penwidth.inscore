
/ITL/scene/* del;

/ITL/scene scale 0.9;

/ITL/scene/rect set rect 0.8 0.1;
/ITL/scene/rect penWidth 5;
/ITL/scene/rect radius 8 5;
/ITL/scene/rect alpha 50;

/ITL/scene/ell set ellipse 0.8 0.4;
/ITL/scene/ell color 200 200 200;
/ITL/scene/ell penWidth 5;
/ITL/scene/ell y 0.3;

/ITL/scene/line set line wa 2 0;
/ITL/scene/line z 1;

/ITL/scene/center set ellipse 0.05 0.05;
/ITL/scene/center red 255;

/ITL/scene/comment set txt "Text";
/ITL/scene/comment fontSize 32;
/ITL/scene/comment y 0.65;
/ITL/scene/comment penWidth 0;

/ITL/scene/line2 set line wa 2 90;
/ITL/scene/line3 set line wa 0.18 0;
/ITL/scene/line3 y 0.65;

/ITL/scene/line* penColor 0 0 255;


__END__
/ITL/scene/line* penWidth 10;


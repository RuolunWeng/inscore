/ITL/scene reset ;

/ITL/scene/comment set txt "system mapping test score" ;
/ITL/scene/comment xorigin -1.0 ;
/ITL/scene/comment yorigin -1.0 ;
/ITL/scene/comment x -0.95 ;
/ITL/scene/comment y -0.95 ;
/ITL/scene/score set gmnf "system.gmn" ;
/ITL/scene/score map system ;
/ITL/scene/score map voice1 ;
/ITL/scene/score map voice2 ;
/ITL/scene/score map voice3 ;
/ITL/scene/score map voice4 ;
/ITL/scene/score/debug map 1 ;
/ITL/scene/score columns 4 ;
/ITL/scene/score scale 2.2 ;
/ITL/scene width 2.3 ;

/ITL/scene watch endPaint ( /ITL/scene export "system.pdf" );

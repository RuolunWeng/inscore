/ITL/scene/_cursor	watch+ 	"timeEnter" 1 1 2 1 	"/ITL/scene/page2" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 1 1 2 1 	"/ITL/scene/page3" "xorigin" -1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 1 1 2 1 	"/ITL/scene/page3" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 1 1 2 1 	"/ITL/scene/_pagepos" "date" 1 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 1 1 2 1 	"/ITL/scene/_pagenum""set"  "txt" 2

/ITL/scene/_cursor	watch 	"timeEnter" 2 1 3 1 	"/ITL/scene/page*" "show" 0
/ITL/scene/_cursor	watch+ 	"timeEnter" 2 1 3 1 	"/ITL/scene/page3" "xorigin" 1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 2 1 3 1 	"/ITL/scene/page3" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 2 1 3 1 	"/ITL/scene/page4" "xorigin" -1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 2 1 3 1 	"/ITL/scene/page4" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 2 1 3 1 	"/ITL/scene/_pagepos" "date" 2 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 2 1 3 1 	"/ITL/scene/_pagenum""set"  "txt" 3

/ITL/scene/_cursor	watch 	"timeEnter" 3 1 4 1 	"/ITL/scene/page*" "show" 0
/ITL/scene/_cursor	watch+ 	"timeEnter" 3 1 4 1 	"/ITL/scene/page4" "xorigin" 1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 3 1 4 1 	"/ITL/scene/page4" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 3 1 4 1 	"/ITL/scene/page5" "xorigin" -1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 3 1 4 1 	"/ITL/scene/page5" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 3 1 4 1 	"/ITL/scene/_pagepos" "date" 3 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 3 1 4 1 	"/ITL/scene/_pagenum" "set"  "txt" 4

/ITL/scene/_cursor	watch 	"timeEnter" 4 1 5 1 	"/ITL/scene/page*" "show" 0
/ITL/scene/_cursor	watch+ 	"timeEnter" 4 1 5 1 	"/ITL/scene/page5" "xorigin" 1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 4 1 5 1 	"/ITL/scene/page5" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 4 1 5 1 	"/ITL/scene/page6" "xorigin" -1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 4 1 5 1 	"/ITL/scene/page6" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 4 1 5 1 	"/ITL/scene/_pagepos" "date" 4 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 4 1 5 1 	"/ITL/scene/_pagenum" "set"  "txt" 5

/ITL/scene/_cursor	watch 	"timeEnter" 5 1 6 1 	"/ITL/scene/page*" "show" 0
/ITL/scene/_cursor	watch+ 	"timeEnter" 5 1 6 1 	"/ITL/scene/page6" "xorigin" 1.
/ITL/scene/_cursor	watch+ 	"timeEnter" 5 1 6 1 	"/ITL/scene/page6" "show" 1
/ITL/scene/_cursor	watch+ 	"timeEnter" 5 1 6 1 	"/ITL/scene/page7" "xorigin" -1.

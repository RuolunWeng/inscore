/ITL/scene/_cursor	watch+ 	"timeEnter" 6 1 7 1 	"/ITL/scene/_pagenum" "set"  "txt" 7



%-------------------------------------------------------
% left and right triangles 
%-------------------------------------------------------
/ITL/scene/_anext      set polygon 0. 0.  0.04 0.02 0. 0.04
/ITL/scene/_aprevious  set polygon 0. 0. -0.04 0.02 0. 0.04
/ITL/scene/_a* color 180 180 180
/ITL/scene/_a* y 0.88
/ITL/scene/_anext x 0.65
/ITL/scene/_aprevious x -0.65

/ITL/scene/_anext 	 watch "mouseDown" 	"localhost:7101/next" 1
/ITL/scene/_aprevious watch "mouseDown"  "localhost:7101/prev" 1
/ITL/scene/_a* 		 watch "mouseEnter" "/ITL/$scene/$self" "color" 255 0 0
/ITL/scene/_a* 		 watch "mouseLeave" "/ITL/$scene/$self" "color" 180 180 180

/ITL/scene/_anext 	 	watch+ "mouseDown" 	"/ITL/scene/_cursor" "ddate" 1 1
/ITL/scene/_aprevious 	watch+ "mouseDown" 	"/ITL/scene/_cursor" "ddate" -1 1

%-------------------------------------------------------
% put all the user interface in front
/ITL/scene/_* 	z 2.0

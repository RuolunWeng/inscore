/ITL/scene/comment1 y -0.6;
/ITL/scene/comment1 scale 1.6;

/ITL/scene/polygon set polygon -0.5 0 0.5 0 0 0.5;
/ITL/scene/polygon alpha 50;

r = 0.03;

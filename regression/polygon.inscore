/ITL/scene/polygon set polygon 	0 0.2 
								0.2 -0.1 
								-0.4 0.3
								0.5 -0.4
								-1 0.7
								0.1 -0.2
								;
/ITL/scene/polygon brushStyle none;
/ITL/scene/polygon penWidth 1;


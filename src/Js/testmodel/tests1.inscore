/ITL/scene new;
gray = 100;
/ITL/scene color $gray $gray $gray ;

/ITL/scene/rect set rect 1 1 ;
/ITL/scene/rect get ;
/ITL/scene/rect get width ;
/ITL/scene/rect2 set rect 0.3 0.5 ;
/ITL/scene/rect2 get height ;
/ITL/scene/rect2 color 255 100 100 ;
/ITL/scene/rect3 set ellipse 2 2 ;
/ITL/scene/curve set curve 1 1 2 2 3 3 4 4 ;
/ITL/scene/curve get;

/ITL/scene/curve set txt hello world;
/ITL/scene/curve get;
/ITL/scene get color;
/ITL/log get width height;

/ITL/scene/img set img toto.png;
/ITL/scene/img get;


#/ITL save;

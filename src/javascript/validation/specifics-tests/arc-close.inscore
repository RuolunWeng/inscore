/ITL/scene/* del;

/ITL/scene/txt1 set txt "Open";
/ITL/scene/txt1 x -0.5;
/ITL/scene/txt1 y -0.8;
/ITL/scene/arc1 set arc 0.5 0.5 0 270;
/ITL/scene/arc1 x -0.5;
/ITL/scene/arc1 y -0.5;
/ITL/scene/arc1 color 100 255 100 100;


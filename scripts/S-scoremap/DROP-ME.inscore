
/ITL/scene/*  del;


/ITL/scene/title  set  txt "This script illustrates symbolic score mappings.";
/ITL/scene/title  y -0.92;
/ITL/scene/title scale 1.7;

/ITL/scene/score  set  gmnf "moz581.gmn";
/ITL/scene/score scale 1.9;
/ITL/scene/score map staff3;
/ITL/scene/score map staff5;
/ITL/scene/score/debug map 1;

/ITL/scene/comment set  txt "Only staves 1 (default), 3 and 5 have a mapping.
You can drop 'systemMap.inscore' to change the mapping";
/ITL/scene/comment y 0.92;


/ITL/scene/rect1/txt set txt "/ITL/scene/rect1/sync slave subrect0;
/ITL/scene/rect1/sync slave subrect1 h;
/ITL/scene/rect1/sync slave subrect2 v;
/ITL/scene/rect1/sync slave subrect3 hv;
/ITL/scene/rect1/sync slave subell4 hv;";

/ITL/scene/rect1/txt color 250 250 250;
/ITL/scene/rect1/txt watch mouseDown (/ITL/scene/rect1/sync slave subrect0,
/ITL/scene/rect1/sync slave subrect1 h,
/ITL/scene/rect1/sync slave subrect2 v,
/ITL/scene/rect1/sync slave subrect3 hv,
/ITL/scene/rect1/sync slave subell4 hv);
/ITL/scene/rect1/txt watch mouseEnter (/ITL/scene/rect1/txt color 200 0 100);
/ITL/scene/rect1/txt watch mouseLeave (/ITL/scene/rect1/txt color 250 250 250);

/ITL/scene/rect1/txt2 set txt "/ITL/scene/rect1/sync slave;";
/ITL/scene/rect1/txt2 color 250 250 250;
/ITL/scene/rect1/txt2 y 0.4;

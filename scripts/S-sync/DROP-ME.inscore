/ITL/scene/rect1/slave/child2 y -0.;

/ITL/scene/rect1/subrect0 set rect 1. 1.5;
/ITL/scene/rect1/subrect0 x -1.1;
/ITL/scene/rect1/subrect0 y -1.4;
/ITL/scene/rect1/subrect0 color 200 200 200;

/ITL/scene/rect1/subrect1 set rect 1. 1.5;

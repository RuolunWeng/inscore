/ITL/scene/rect1/txt2 watch mouseEnter (/ITL/scene/rect1/txt2 color 200 0 100);
/ITL/scene/rect1/txt2 watch mouseLeave (/ITL/scene/rect1/txt2 color 250 250 250);

/ITL/scene/rect1/subrect0/txt set txt "sync slave subrect0 del;";
/ITL/scene/rect1/subrect0/txt y 1.2;

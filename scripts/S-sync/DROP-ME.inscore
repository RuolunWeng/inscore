/ITL/scene/rect1/subell4/txt set txt "sync slave subrect0 del;";
/ITL/scene/rect1/subell4/txt y 1.3;
/ITL/scene/rect1/subell4/txt watch mouseEnter (/ITL/scene/rect1/subell4/txt color 200 0 100);
